<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Hosting Session Spot Children </h5>
            </div>
            <div class="ibox-content">

                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th>Child</th>
                        <th>Hosting session spot</th>
                                                <th></th>
                    </tr>
                    </thead>
                    <tbody>
                        <% @hosting_session_spot_children.each do |hosting_session_spot_child| %>
                        <tr>
                            <td><%= hosting_session_spot_child.child_id %></td>
                            <td><%= hosting_session_spot_child.hosting_session_spot_id %></td>
                                                        <td>
                                <div class="btn-group">
                                    <%= link_to hosting_session_spot_child, class: 'btn btn-sm btn-outline dim btn-primary' do %>
                                        <i class="fa fa-search"></i>
                                    <% end %>
                                    <%= link_to edit_hosting_session_spot_child_path(hosting_session_spot_child), class: 'btn btn-sm btn-outline dim btn-info' do %>
                                        <i class="fa fa-edit"></i>
                                    <% end %>
                                    <%= link_to hosting_session_spot_child, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-sm btn-outline dim btn-danger' do %>
                                        <i class="fa fa-trash-o"></i>
                                    <% end %>
                                </div>
                            </td>
                        </tr>
                        <% end %>
                    </tbody>
                </table>
                <hr>
                <div class="btn">
                    <%= link_to new_hosting_session_spot_child_path, class: 'btn btn-sm btn-outline dim btn-primary' do %>
                        <i class="fa fa-plus"></i> New Hosting Session Spot Child
                    <% end %>
                </div>
            </div>
        </div>
    </div>
</div>