<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Hosting Periods ID #<%= @hosting_period.id %></h5>
            </div>
            <div class="ibox-content">
                                <p>
                    <strong>Name:</strong>
                    <%= @hosting_period.name %>
                </p>

                                <p>
                    <strong>Begins:</strong>
                    <%= @hosting_period.begins %>
                </p>

                                <p>
                    <strong>Visible:</strong>
                    <%= @hosting_period.visible %>
                </p>

                                
                <hr>
                <div class="btn">
                    <%= link_to hosting_periods_path, class: 'btn btn-sm btn-outline dim btn-primary' do %>
                        <i class="fa fa-chevron-left"></i> Back
                    <% end %>
                    <%= link_to edit_hosting_period_path(@hosting_period), class: 'btn btn-sm btn-outline dim btn-primary' do %>
                        <i class="fa fa-edit"></i> Edit Hosting Period
                    <% end %>
                </div>
            </div>
        </div>
    </div>
</div>